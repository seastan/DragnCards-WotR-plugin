            ["LOAD_CARDS", "Hobbit"],
            ["SET", "/playerData/$PLAYER_N/label", "Gandalf"],
            ["SET", "/textBoxById/p1Label/content", "Gandalf"],
            ["DEFINE", "$PLAYER_N", "player2"],
