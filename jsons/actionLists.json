            ["SET", "/numPlayers", 3],
            ["SET_NUM_BLUE_RED", 1, 2],
            ["SET", "/cardsPerRound", {"player1": 6, "player2": 4, "player3": 4}]
        ],
        "coopThreePlayerSetup": [
            ["SET", "/dualMode", true],
            ["LOAD_CARDS", "AtS-Paths"],
            ["LOAD_CARDS", "AtS-Battlegrounds"],
            ["DEFINE", "$PLAYER_N", "player1"],
            ["LOAD_CARDS", "Hobbit"],
            ["SET", "/playerData/$PLAYER_N/label", "Gandalf"],
            ["SET", "/textBoxById/p1Label/content", "Gandalf"],
            ["DEFINE", "$PLAYER_N", "player2"],
            ["LOAD_CARDS", "AtS-Sauron"],
            ["SET", "/playerData/$PLAYER_N/label", "W-k"],
            ["SET", "/textBoxById/p2Label/content", "W-k"],
            ["SET", "/playerData/$PLAYER_N/ringTokens", 0],
            ["DEFINE", "$PLAYER_N", "player1"],
            ["LOAD_CARDS", "Aragorn"],
            ["DEFINE", "$PLAYER_N", "player3"],
            ["LOAD_CARDS", "AtS-Isengard"],
            ["SET", "/playerData/$PLAYER_N/label", "Saruman"],
            ["SET", "/textBoxById/p3Label/content", "Saruman"],
	        ["SET", "/playerData/$PLAYER_N/ringTokens", 0],
	        ["SET", "/groupById/player2Reserve/onCardEnter/rotation", 0],
	        ["SET", "/groupById/player3Reserve/onCardEnter/rotation", 0],
            ["LOAD_CARDS", "Duel Rules"],
            ["LOAD_CARDS", "AtS-Rulebook"],
            ["LOAD_CARDS", "Rulebook"],
            ["SET", "/numPlayers", 3],
            ["SET_NUM_BLUE_RED", 1, 2],
            ["SET", "/cardsPerRound", {"player1": 6, "player2": 4, "player3": 4}],
